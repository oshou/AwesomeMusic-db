version: "2"
services:
  mysql:
    container_name: mysql
    build: .
    ports:
      - 3306:3306
    environment:
      MYSQL_ROOT_PASSWORD: $MYSQL_ROOT_PASSWORD
      MYSQL_DATABASE: $MYSQL_DATABASE
      TZ: $MYSQL_TIMEZONE
    working_dir: /var/lib/mysql
    command: mysqld
    #volumes:
    #  - ./db:/var/lib/mysql
